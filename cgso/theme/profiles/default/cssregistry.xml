<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_css tool. -->

<object name="portal_css">
    <stylesheet title=""
        id="public.css"
        media="screen" rel="stylesheet" rendering="import"
        cacheable="True" compression="safe" cookable="True"
        enabled="1" expression="not: request/HTTP_X_THEME_ENABLED | nothing"/>

    <!-- -*- extra stuff goes here -*- -->


 <stylesheet title=""
     id="++theme++cgso.theme/css/public.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression="request/HTTP_X_THEME_ENABLED|nothing"/>

 <stylesheet title=""
     id="++theme++cgso.theme/css/style.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    insert-after="++theme++cgso.theme/css/public.css"
    enabled="1" expression="request/HTTP_X_THEME_ENABLED|nothing"/>


</object>
